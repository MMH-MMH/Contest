<script src = "/socket.io/socket.io.js"></script>
<script>
    var socket = io();
    socket.on('change', (data) => {
        var id = data.id;
        $('#verdict'+id).html('');
        $('#verdict'+id).html(data.verdict);
        $('#time'+id).html('');
        $('#time'+id).html(data.time+"ms");
        $('#memory'+id).html('');
        $('#memory'+id).html(data.memory+"KB");

    })


</script>

<p id = "demo"></p>

<table style="margin: auto;" id="subtable" border = "1px solid #000">
    <thead>
        <tr>
            <th>S_ID</th>
            <th>User</th>
