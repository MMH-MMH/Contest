</div>


<br />
Displaying <z id="noss"></z> results per page<br />
<br />
Page <z id="lpages"></z> of <z id="llasts"></z>
<br />

<script>

    
    $(document).ready(() => {
        var cp = window.location.pathname;
        cp = cp.toString().trim();
        cp = cp.split('/');
        cp = cp[cp.length-1];
        
    
        $.ajax({
            
            url: '/users/{{user}}/getsub/'+cp,
            success: (res) => {
                console.log(res);
                var st = $("#subdet");
                st.html('');
                var data = res.subs, user = res.user;
                for(let i = 0; i < data.length; i++){

                    st.append('\
                        <tr>\
                            <td><a href="/users/'+data[i][1]+'/'+data[i][0]+'">' + data[i][0] + '</a></td>\
                            <td>' + data[i][1] + '</td>\
                            <td>' + data[i][2] + '</td>\
                            <td>' + data[i][3] + '</td>\
                            <td class="verd" id = "verdict'+data[i][0]+'">' + data[i][4] + '</td>\
                            <td id = "time'+data[i][0]+'">' + data[i][5] + 'ms</td>\
                            <td id = "memory'+data[i][0]+'">' + data[i][6] + 'KB</td>\
                        </tr>\
                    ');
                    var y="verdict"+data[i][0];
                    var x=data[i][4];
                    if(x=="Accepted"){
                        document.getElementById(y).style.color="green";
                    }else if(x=="Compiling" || x=="In queue"){
                        document.getElementById(y).style.color="blue";
                    }
                    else{
                        document.getElementById(y).style.color="red";
                    }
                    
                }

                var sp = $("#subPagination"), lo = $("#loaders");
                sp.html('');
                lo.empty();

                if(res.checkp){
                    if(res.checkm){
                        sp.append('\
                            <ul class="pagination justify-content-center">\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/1" value="1"> First </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.prev+'" value="'+res.prev+'"> &laquo; </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.midp+'" value="'+res.midp+'"> '+res.midp+' </a></li>\
                                <li class="page-item active"><a class="page-link" href = "/users/'+user+'/submissions/'+res.page+'" value="'+res.page+'"> '+res.page+' </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.next+'" value="'+res.next+'"> &raquo; </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.mid+'" value="'+res.mid+'"> '+res.mid+' </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.last+'" value="'+res.last+'"> Last </a></li>\
                            </ul>\
                        ');
                    } else {
                        sp.append('\
                            <ul class="pagination justify-content-center">\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/1" value="1"> First </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.prev+'" value="'+res.prev+'"> &laquo; </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.midp+'" value="'+res.midp+'"> '+res.midp+' </a></li>\
                                <li class="page-item active"><a class="page-link" href = "/users/'+user+'/submissions/'+res.page+'" value="'+res.page+'"> '+res.page+' </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.next+'" value="'+res.next+'"> &raquo; </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.last+'" value="'+res.last+'"> Last </a></li>\
                            </ul>\
                        ');
                    }
                } else {
                    if(res.checkm){
                        sp.append('\
                            <ul class="pagination justify-content-center">\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/1" value="1"> First </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.prev+'" value="'+res.prev+'"> &laquo; </a></li>\
                                <li class="page-item active"><a class="page-link" href = "/users/'+user+'/submissions/'+res.page+'" value="'+res.page+'"> '+res.page+' </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.next+'" value="'+res.next+'"> &raquo; </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.mid+'" value="'+res.mid+'"> '+res.mid+' </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.last+'" value="'+res.last+'"> Last </a></li>\
                            </ul>\
                        ');
                    } else {
                        sp.append('\
                            <ul class="pagination justify-content-center">\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/1" value="1"> First </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.prev+'" value="'+res.prev+'"> &laquo; </a></li>\
                                <li class="page-item active"><a class="page-link" href = "/users/'+user+'/submissions/'+res.page+'" value="'+res.page+'"> '+res.page+' </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.next+'" value="'+res.next+'"> &raquo; </a></li>\
                                <li class="page-item"><a class="page-link" href = "/users/'+user+'/submissions/'+res.last+'" value="'+res.last+'"> Last </a></li>\
                            </ul>\
                        ');
                    }
                }

                $("#noss").html('');
                $("#llasts").html('');
                $("#lpages").html('');

                $("#noss").append(res.nos);
                $("#llasts").append(res.last);
                $("#lpages").append(res.page);

            }
        })

    });
    
    

    

</script>